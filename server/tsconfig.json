{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "removeComments": true,
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "Node",
  }
}